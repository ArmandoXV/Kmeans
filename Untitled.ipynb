    "import math\n",
    "\n",
    "def Average(x):\n",
    "    sum =float(0)\n",
    "    for i in x:\n",
    "        sum+=i\n",
    "    return sum/len(x)\n",
    "\n",
    "def Distance(x,y):\n",
    "    return math.sqrt((x[0]-y[0])*(x[0]-y[0]) + (x[1]-y[1])*(x[1]-y[1]))\n"
